import java.util.*;
class primenum
{
	public static void main(String args[]){
	 Scanner sc = new Scanner(System.in);
	 for(int num = 1; num <=100;num++){
		  int count=0;
		for(int j=1; j<=num;j++){
			if(num %j ==0){
				count++;
			}
		}
	
	 if(count==2){
		System.out.println(num +"is prime");
	 }
	  }
	}
}
