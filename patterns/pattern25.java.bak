public class pattern25
{
	public static void main(String args[]){
		
		int n=4;
		char ch='A';
		int count=1;
		for(int i=1; i<=n;i++){
			for(int j=1;j<=n/2;j++){
				System.out.print(ch+" "+ count+" ");
				count++;
				ch++;
			}
			System.out.println();
		}
	}
}

